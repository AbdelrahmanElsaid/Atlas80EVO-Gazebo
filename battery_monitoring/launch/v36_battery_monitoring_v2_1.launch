<?xml version="1.0"?>

<launch>
    <!-- 36V Battery Monitoring -->
    <node pkg="battery_monitoring" type="v36_battery_monitoring_v2_1.py" name="v36_battery_monitoring_v2_1" output="screen">
        <param name="voltage_topic" value="battery/voltage"/>
        <param name="battery_topic" value="battery/percent"/>
        <param name="volt_36_110" value="41.0"/>
        <param name="volt_36_105" value="40.0"/>
        <param name="volt_36_100" value="39.0"/>
        <param name="volt_36_95" value="38.8"/>
        <param name="volt_36_90" value="38.6"/>
        <param name="volt_36_85" value="38.4"/>
        <param name="volt_36_80" value="38.2"/>
        <param name="volt_36_75" value="38.0"/>
        <param name="volt_36_70" value="37.8"/>
        <param name="volt_36_65" value="37.6"/>
        <param name="volt_36_60" value="37.4"/>
        <param name="volt_36_55" value="37.2"/>
        <param name="volt_36_50" value="37.0"/>
        <param name="volt_36_45" value="36.8"/>
        <param name="volt_36_40" value="36.6"/>
        <param name="volt_36_35" value="36.4"/>
        <param name="volt_36_30" value="36.2"/>
        <param name="volt_36_25" value="36.0"/>
        <param name="volt_36_20" value="35.8"/>
        <param name="volt_36_15" value="35.0"/>
        <param name="volt_36_10" value="33.0"/>
        <param name="volt_36_5" value="31.0"/>
        <param name="volt_36_0" value="28.0"/>
        <param name="volt_tolerance" value="0.0"/> 
        <!-- "+" <-> higher than actual    |    "-" <-> lower than actual -->
        <param name="weight" value="0.5"/>
    </node>

    <!-- Battery GUI -->
<!--    <node pkg="battery_monitoring" type="battery_gui.py" name="battery_gui"/> -->
</launch>
