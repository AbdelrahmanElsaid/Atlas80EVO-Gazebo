<?xml version="1.0"?>

<launch>
    <!-- 36V Battery Monitoring -->
    <node pkg="battery_monitoring" type="v36_battery_monitoring.py" name="v36_battery_monitoring">
        <param name="voltage_topic" value="battery/voltage"/>
        <param name="battery_topic" value="battery/percent"/>
        <param name="volt_36_100" value="41.0"/>
        <param name="volt_36_95" value="40.5"/>
        <param name="volt_36_90" value="40.2"/>
        <param name="volt_36_85" value="39.9"/>
        <param name="volt_36_80" value="39.6"/>
        <param name="volt_36_75" value="39.3"/>
        <param name="volt_36_70" value="39.0"/>
        <param name="volt_36_65" value="38.7"/>
        <param name="volt_36_60" value="38.4"/>
        <param name="volt_36_55" value="38.1"/>
        <param name="volt_36_50" value="37.8"/>
        <param name="volt_36_45" value="37.5"/>
        <param name="volt_36_40" value="37.2"/>
        <param name="volt_36_35" value="36.9"/>
        <param name="volt_36_30" value="36.6"/>
        <param name="volt_36_25" value="36.3"/>
        <param name="volt_36_20" value="36.0"/>
<!--        <param name="volt_36_18" value="34.4"/>
        <param name="volt_36_16" value="33.8"/>
        <param name="volt_36_14" value="33.2"/>
        <param name="volt_36_12" value="32.6"/> -->
        <param name="volt_36_15" value="35.0"/>
        <param name="volt_36_10" value="34.0"/>
        <param name="volt_36_5" value="31.0"/>
        <param name="volt_36_0" value="28.0"/>
        <param name="volt_tolerance" value="0.0"/> 
        <!-- "+" <-> higher than actual    |    "-" <-> lower than actual -->
    </node>

    <!-- Battery GUI -->
<!--    <node pkg="battery_monitoring" type="battery_gui.py" name="battery_gui"/> -->
</launch>
